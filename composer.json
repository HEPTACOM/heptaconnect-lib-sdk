{
    "name": "heptacom/heptaconnect-lib-sdk",
    "type": "library",
    "description": "Library for the Software Development Kit for HEPTAconnect",
    "keywords": [
        "heptaconnect",
        "development",
        "sdk"
    ],
    "version": "0.4.0",
    "license": [
        "AGPL-3.0-or-later",
        "proprietary"
    ],
    "authors": [
        {
            "name": "HEPTACOM GmbH"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "bentools/iterable-functions": "^1.4",
        "composer/composer": "^2",
        "composer/package-versions-deprecated": "^1.8",
        "doctrine/dbal": ">=2.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "heptacom/heptaconnect-bridge-shopware-platform": "self.version",
        "heptacom/heptaconnect-core": "self.version",
        "heptacom/heptaconnect-portal-base": "self.version",
        "heptacom/heptaconnect-storage-base": "self.version",
        "psy/psysh": "@stable",
        "shopware/administration": "6.4.1.2",
        "shopware/core": "6.4.1.2",
        "shopware/storefront": "6.4.1.2",
        "symfony/config": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0"
    },
    "conflict": {
        "nikic/php-parser": "4.7.0"
    },
    "require-dev": {
        "dq5studios/psalm-junit": ">=2",
        "ergebnis/composer-normalize": ">=2.7",
        "icanhazstring/composer-unused": ">=0.7",
        "infection/infection": ">=0.21",
        "maglnet/composer-require-checker": ">=2",
        "mavimo/phpstan-junit": ">=0.3",
        "phpstan/extension-installer": ">=1",
        "phpstan/phpstan": ">=0.12",
        "phpstan/phpstan-deprecation-rules": ">=0.12",
        "phpstan/phpstan-phpunit": ">=0.12",
        "phpstan/phpstan-strict-rules": ">=0.12",
        "phpunit/phpunit": ">=9.3 <10",
        "psalm/plugin-phpunit": ">=0.10",
        "vimeo/psalm": ">=4"
    },
    "autoload": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heptacom\\HeptaConnect\\Test\\": "test/"
        }
    },
    "repositories": {
        "heptaconnect-sources": {
            "type": "path",
            "url": "../**"
        }
    },
    "support": {
        "issues": "https://github.com/HEPTACOM/heptaconnect-lib-sdk/issues",
        "source": "https://github.com/HEPTACOM/heptaconnect-lib-sdk",
        "docs": "https://connect.heptacom.de/",
        "rss": "https://connect.heptacom.de/feed/rss2.xml"
    },
    "bin": [
        "bin/heptaconnect-sdk"
    ]
}
